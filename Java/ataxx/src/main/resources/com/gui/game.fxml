                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
            </GridPane>
         </children>
      </AnchorPane>
   </top>
   <center>
      <Button fx:id="ResetGameButton" alignment="BOTTOM_CENTER" maxHeight="25.0" maxWidth="104.0" minHeight="25.0" minWidth="79.0" mnemonicParsing="false" onAction="#ResetGame" prefHeight="25.0" prefWidth="104.0" text="Reset Game" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
